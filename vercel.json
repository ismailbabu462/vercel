{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://pentorsec-api-289572488966.us-central1.run.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://pentorsec-api-289572488966.us-central1.run.app/api",
    "REACT_APP_BACKEND_URL": "https://pentorsec-api-289572488966.us-central1.run.app"
  }
}
